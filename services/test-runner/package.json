{
  "name": "test-runner",
  "version": "1.0.0",
  "description": "Stagehand v3 test runner service for Kiwi",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^3.0.7",
    "express": "^4.18.2",
    "bullmq": "^5.1.7",
    "ioredis": "^5.3.2",
    "@supabase/supabase-js": "^2.84.0",
    "zod": "^3.22.4",
    "dotenv": "^16.4.7",
    "winston": "^3.11.0",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "typescript": "^5.9.3",
    "tsx": "^4.19.2",
    "eslint": "^9.0.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

